int degree = 10


int n = 0
int fact_tmp = 1
int fact_total = 1

bool cont = true
bool fact_cont = true

while cont
  cont = degree > 1

  fact_tmp = n
  fact_total = 1
  fact_cont = 1 < fact_tmp
  while fact_cont
    fact_cont = 1 < fact_tmp
    fact_total = fact_total * fact_tmp
    fact_tmp = fact_tmp - 1
  done
  fact_cont = true

  print fact_total

  n = n + 1
  degree = degree - 1
done
